{
    "name": "tonis-io/doctrine-orm",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Tonis\\DoctrineORM\\": [
                "src/"
            ]
        }
