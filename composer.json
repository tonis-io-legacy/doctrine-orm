    "autoload-dev": {
        "psr-4": {
            "Tonis\\DoctrineORM\\": [
                "test/"
            ]
        }
    },
    "bin": [
        "bin/doctrine.php"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
